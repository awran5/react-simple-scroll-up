{
  "name": "react-simple-scroll-up",
  "version": "0.2.0",
  "description": "A minimal lightweight react component for adding a nice scroll up (back to top) button with onScroll progress.",
  "author": "awran5 <awran5@yahoo.com>",
  "license": "MIT",
  "repository": "https://github.com/awran5/react-simple-scroll-up",
  "bugs": "https://github.com/awran5/react-simple-scroll-up/issues",
  "homepage": "https://github.com/awran5/react-simple-scroll-up#readme",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "clean": "rm -rf dist",
    "start:rollup": "rollup -cw --exports auto",
    "start:example": "cd example && yarn start",
    "start": "run-p start:rollup start:example",
    "build": "rollup -c --exports auto",
    "predeploy": "cd example && yarn && yarn run build",
    "deploy": "gh-pages -d example/build",
    "publish": "npm publish",
    "release": "standard-version"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.58.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-version": "^9.3.2",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "react",
    "react component",
    "to top",
    "scroll up",
    "progess on scroll"
  ],
  "tags": [
    "react",
    "react-component",
    "to-top",
    "scroll-up",
    "progess-on-scroll"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {}
}
